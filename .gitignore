# Agent runtime state (don't commit, changes constantly)
/shared-state/agent-status.json
/shared-state/locks/*
!/shared-state/locks/.gitkeep
/shared-state/communication.md
/logs/active/*
!/logs/active/.gitkeep
*.log

# Environment and secrets
.env
.env.local
**/.mcp-secrets/

# OS and IDE
.DS_Store
.vscode/
.idea/

# Dependencies
node_modules/
__pycache__/
*.pyc
venv/
.venv/

# Build outputs
dist/
build/
*.egg-info/

# Multi-team workspace (team-specific working directories)
/*-workspace/
!shared-workspace/

# Team-specific logs
*-workspace/logs/
*-workspace/temp/

# Project workspaces (runtime data created by initialize.sh)
/projects/
!projects/.gitkeep

# Launch prompt artifacts
.launch-prompt.md
